<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navi"
    app:startDestination="@id/musicFragment">

    <fragment
        android:id="@+id/musicFragment"
        android:name="com.example.base.MusicFragment"
        android:label="fragment_music"
        tools:layout="@layout/fragment_music" >
        <action
            android:id="@+id/action_musicFragment_to_messangerFragment"
            app:destination="@id/messangerFragment" />
        <action
            android:id="@+id/action_musicFragment_to_coming_soon"
            app:destination="@id/coming_soon" />
    </fragment>
    <fragment
        android:id="@+id/messangerFragment"
        android:name="com.example.base.MessangerFragment"
        android:label="fragment_messanger"
        tools:layout="@layout/fragment_messanger" >
        <action
            android:id="@+id/action_messangerFragment_to_musicFragment"
            app:destination="@id/musicFragment" />
        <action
            android:id="@+id/action_messangerFragment_to_coming_soon"
            app:destination="@id/coming_soon" />
    </fragment>
    <fragment
        android:id="@+id/coming_soon"
        android:name="com.example.base.coming_soon"
        android:label="fragment_coming_soon"
        tools:layout="@layout/fragment_coming_soon" >
        <action
            android:id="@+id/action_coming_soon_to_musicFragment"
            app:destination="@id/musicFragment" />
        <action
            android:id="@+id/action_coming_soon_to_messangerFragment"
            app:destination="@id/messangerFragment" />
    </fragment>
</navigation>